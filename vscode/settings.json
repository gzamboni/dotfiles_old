{
  "editor.fontSize": 15,
  "editor.formatOnSave": true,
  "editor.rulers": [
    120
  ],
  "editor.tabSize": 2,
  "editor.trimAutoWhitespace": true,
  "editor.tabCompletion": "on",
  "explorer.openEditors.visible": 0,
  "window.restoreWindows": "none",
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.fontSize": 14,
  "files.autoSave": "onFocusChange",
  "extensions.autoUpdate": true,
  "editor.cursorBlinking": "phase",
  "window.title": "${activeEditorMedium}${separator}${rootName}",
  "files.trimTrailingWhitespace": true,
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "boundary",
  "workbench.colorTheme": "MacOS Modern Dark - Ventura Xcode",
  "extensions.ignoreRecommendations": false,
  "go.coverageDecorator": {
    "type": "gutter"
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "/vendor": true
  },
  "workbench.startupEditor": "newUntitledFile",
  "workbench.fontAliasing": "auto",
  "workbench.editor.showIcons": true,
  "explorer.confirmDragAndDrop": false,
  "search.smartCase": true,
  "files.associations": {
    "*.tmpl": "gotemplate"
  },
  "go.testTimeout": "3m",
  "breadcrumbs.enabled": true,
  "go.formatTool": "goimports",
  "search.useGlobalIgnoreFiles": true,
  "workbench.editor.highlightModifiedTabs": true,
  "search.showLineNumbers": true,
  "ruby.intellisense": "rubyLocate",
  "ruby.format": "rubocop",
  "ruby.codeCompletion": "rcodetools",
  "ruby.useLanguageServer": true,
  "terminal.integrated.cursorBlinking": true,
  "go.coverOnSingleTest": true,
  "terminal.integrated.cursorStyle": "line",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "go.useLanguageServer": true,
  "go.alternateTools": {
    "go-langserver": "gopls"
  },
  "go.languageServerExperimentalFeatures": {
    "format": true,
    "autoComplete": true
  },
  "window.newWindowDimensions": "inherit",
  "go.buildFlags": [
    "-v"
  ],
  "editor.smoothScrolling": true,
  "files.exclude": {
    "yarn.lock": true
  },
  "tabnine.experimentalAutoImports": true,
  "yaml.schemas": {
    "file:///Users/gzamboni/.vscode/extensions/atlassian.atlascode-3.0.3/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
  },
  "editor.inlineSuggest.enabled": true,
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "terraform.languageServer.enable": true,
  "redhat.telemetry.enabled": true,
  "githubPullRequests.pullBranch": "never",
  "editor.lineNumbers": "relative",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.productIconTheme": "macos-modern",
  "workbench.iconTheme": "macos-modern-big-sur-icon-theme",
  "settingsSync.ignoredSettings": [
    "-python.venvFolders",
    "-direnv.path.executable"
  ],
  "atlascode.jira.jqlList": [
    {
      "id": "0bb0315f-a76f-4d09-aded-f28167cd034c",
      "siteId": "b6694e13-1510-4d7f-8e27-be5daf3bab7d",
      "name": "My Issues",
      "query": "project = \"TOTVS LABS - Carol Assistant\" AND assignee = currentUser() and status not in (Closed, Done, Canceled) and type != Epic",
      "enabled": true,
      "monitor": true
    },
    {
      "id": "1cd78ccb-06ba-4930-87dc-a3ef86802a43",
      "siteId": "b6694e13-1510-4d7f-8e27-be5daf3bab7d",
      "name": "TODO - This Sprint",
      "query": "project = \"TOTVS LABS - Carol Assistant\" AND Sprint in openSprints() AND status = \"To do\"",
      "enabled": true,
      "monitor": true
    },
    {
      "id": "1e2718fe-1fea-4c9a-9df8-1540b5585cc8",
      "siteId": "b6694e13-1510-4d7f-8e27-be5daf3bab7d",
      "name": "This Sprint Issue",
      "query": "project = \"TOTVS LABS - Carol Assistant\" AND Sprint in openSprints() and status not in (Closed, Done, Canceled)",
      "enabled": true,
      "monitor": true
    }
  ],
  "atlascode.jira.explorer.nestSubtasks": false,
  "codeium.enableConfig": {
    "*": false,
    "ReactNativeToolsOutput": true,
    "plaintext": true,
    "makefile": true
  },
  "github.copilot.enable": {
    "*": true,
    "yaml": true,
    "plaintext": true,
    "markdown": false,
    "scminput": false
  },
  "git.autofetch": true,
  "liveshare.presence": true,
  "vsicons.dontShowNewVersionMessage": true,
  "terminal.integrated.fontFamily": "'Cascadia Code PL', 'FiraCode Nerd Font Mono', Hack, Menlo, Monaco, 'Courier New', monospace",
  "direnv.path.executable": "",
  "go.toolsManagement.autoUpdate": true,
  "direnv.status.showChangesCount": false,
  "githubPullRequests.remotes": [
    "origin",
    "upstream",
    "github"
  ],
  "githubPullRequests.queries": [
    {
      "label": "Waiting For My Review",
      "query": "is:open review-requested:${user}"
    },
    {
      "label": "Assigned To Me",
      "query": "is:open assignee:${user}"
    },
    {
      "label": "Created By Me",
      "query": "is:open author:${user}"
    }
  ],
  "githubPullRequests.pushBranch": "always",
  "security.workspace.trust.untrustedFiles": "open",
  "yaml.customTags": [
    "!And",
    "!And sequence",
    "!If",
    "!If sequence",
    "!Not",
    "!Not sequence",
    "!Equals",
    "!Equals sequence",
    "!Or",
    "!Or sequence",
    "!FindInMap",
    "!FindInMap sequence",
    "!Base64",
    "!Join",
    "!Join sequence",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!Sub sequence",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!ImportValue sequence",
    "!Select",
    "!Select sequence",
    "!Split",
    "!Split sequence"
  ],
  "jupyter.askForKernelRestart": false,
  "editor.fontFamily": "'FiraCode Nerd Font Mono', Hack, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontVariations": true,
  "editor.fontWeight": "normal",
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "name": "Comment",
        "scope": [
          "comment",
          "comment.block",
          "comment.block.documentation",
          "comment.line",
          "comment.line.double-slash",
          "punctuation.definition.comment",
        ],
        "settings": {
          "fontStyle": "italic",
          "foreground": "#828282"
        }
      },
      {
        "name": "Functions and Methods",
        "scope": [
          "entity.name.function",
          "entity.name.method",
          "entity.name.class",
        ],
		"settings": {
			"fontStyle": "italic",
		  }
      }
    ]
  }
}
